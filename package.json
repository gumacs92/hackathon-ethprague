{
  "name": "hackathon-ethprague",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "lintfix": "npm run lint:js -- --fix",
    "test": "jest",
    "generate-types": "npx typechain --target=truffle-v5 'build/contracts/*.json'",
    "remix": "remixd -u https://remix.ethereum.org/",
    "deploy": "truffle migrate --network development --reset && npm run generate-types"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.6.0",
    "@remix-project/remixd": "^0.6.2",
    "core-js": "^3.19.3",
    "luxon": "^2.4.0",
    "moralis": "^1.8.0",
    "nuxt": "^2.15.8",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@nuxtjs/eslint-config": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/tailwindcss": "^4.2.1",
    "@typechain/truffle-v5": "^8.0.1",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.41",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.4.4",
    "eslint": "^8.4.1",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "^27.4.4",
    "postcss": "^8.4.4",
    "truffle": "^5.5.18",
    "ts-node": "^10.8.1",
    "typechain": "^8.1.0",
    "typescript": "^4.7.3",
    "vue-jest": "^3.0.4"
  }
}
